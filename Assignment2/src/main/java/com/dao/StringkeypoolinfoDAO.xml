<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.StringkeypoolinfoDAO">		
	
	<insert id="saveNewStringKey" parameterType="com.dto.StringkeypoolinfoTDTO">
			INSERT INTO kakao.string_key_pool_info
			(`StrKeyNum`)
			VALUES
			(#{StrKeyNum})
	</insert>	

	<delete id="clearNewStringKey" >
			DELETE FROM kakao.string_key_pool_info
			
	</delete>	
	
	<select id="getNewStringKey" parameterType="long" resultType="com.dto.StringkeypoolinfoTDTO">
			SELECT  * FROM kakao.string_key_pool_info WHERE keySeq >#{keySeq} ORDER BY keySeq LIMIT 1
	</select>	
	
</mapper>
